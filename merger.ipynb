    "\n",
    "import os\n",
    "import glob\n",
    "import pandas as pd\n",
    "\n",
    "%cd ..\n",
    "\n",
    "extension = 'csv'\n",
    "all_filenames = [i for i in glob.glob('*.{}'.format(extension))]\n",
    "\n",
    "combined_csv = pd.concat([pd.read_csv(f) for f in all_filenames ])\n",
    "\n",
    "\n",
    "final_csv = combined_csv\n",
    "\n",
    "\n",
    "\n",
    "final_csv.to_csv('final.csv', index=False)"
   ]
  }
